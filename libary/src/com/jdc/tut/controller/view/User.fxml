<VBox prefHeight="750.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" text="Save" />
            <Button mnemonicParsing="false" text="Search" />
            <Button mnemonicParsing="false" text="Delete" />
         </children>
         <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets right="20.0" />
         </VBox.margin>
      </HBox>
      <GridPane hgap="10.0">
        <columnConstraints>
          <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" maxWidth="775.0" minWidth="10.0" prefWidth="212.0" />
            <ColumnConstraints hgrow="ALWAYS" maxWidth="566.0" minWidth="10.0" prefWidth="279.0" />
            <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" maxWidth="437.0" minWidth="10.0" prefWidth="194.0" />
          <ColumnConstraints hgrow="ALWAYS" maxWidth="345.0" minWidth="10.0" prefWidth="345.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="User Name">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField GridPane.columnIndex="1" />
            <Label text="Password" GridPane.columnIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <PasswordField GridPane.columnIndex="3" />
            <Label text="LoginId" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Confirm" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <PasswordField GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <Label text="UserRole" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ComboBox prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Label" GridPane.columnIndex="3" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets right="30.0" />
         </VBox.margin>
         <padding>
            <Insets right="10.0" top="10.0" />
         </padding>
      </GridPane>
      <TableView prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="75.0" text="No" />
          <TableColumn prefWidth="75.0" text="User Name" />
        </columns>
      </TableView>
   </children>
</VBox>
